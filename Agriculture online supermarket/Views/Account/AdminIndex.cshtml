@model List<Agriculture_online_supermarket.Models.IndexModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "管理界面";
}
<h2>管理界面</h2>
<div class="row">
    <div class="col-md-8">
        @{
            int count = 0;
        }
        @if (Model != null)
        {
            for (int i = 0; i < Math.Ceiling(Model.Count / 1.0); i++)
            {
                Agriculture_online_supermarket.Models.IndexModel model = Model[count];
                <div class="form-group">
                    <a href="UserInfo/@Html.Raw(model.sellerName)">
                        <div class="col-md-10">
                            <input type="submit" value="修改" class="btn btn-default" />
                            <input type="submit" value="删除" class="btn btn-default" />
                        </div>
                    </a>
                </div>
                count++;
            }
        }
    </div>
</div>
